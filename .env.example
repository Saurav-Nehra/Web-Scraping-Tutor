# copy to .env and edit
JIRA_BASE_URL=https://issues.apache.org/jira
# optional: add an email:api_token if you have authenticated access, otherwise public endpoints will be used
JIRA_AUTH_USER=
JIRA_AUTH_TOKEN=
# comma separated projects to scrape
PROJECTS=HADOOP,HIVE,SPARK
# maximum issues per request (Jira max often 100)
MAX_RESULTS=100
# output folder
OUTPUT_DIR=data/processed
RAW_DIR=data/raw
